{
  "schemaVersion": 1,
  "id": "audioplayer",
  "version": "${mod_version}",
  "name": "AudioPlayer",
  "description": "Audio Player",
  "authors": [
    "Max Henkel"
  ],
  "contact": {
    "website": "https://modrepo.de"
  },
  "license": "All Rights Reserved",
  "icon": "icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "de.maxhenkel.audioplayer.AudioPlayerMod"
    ],
    "voicechat": [
      "de.maxhenkel.audioplayer.voicechat.VoicechatAudioPlayerPlugin"
    ]
  },
  "mixins": [
    "audioplayer.mixins.json"
  ],
  "depends": {
    "fabricloader": "${fabric_loader_dependency}",
    "minecraft": "${minecraft_dependency}",
    "voicechat_api": ">=${voicechat_api_version}"
  },
  "breaks": {
    "fabric-api": "${fabric_api_dependency_breaks}"
  },
  "accessWidener": "audioplayer.accesswidener"
}
